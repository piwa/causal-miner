[
  {
    "selector": "node",
    "style": {
      "label": "data(label)",
      "background-color": "data(color)",
      "display": "data(display)",
      "shape": "data(shape)",
      "width": "label",
      "height": 45,
      "padding": 7,
      "border-color": "data(borderColor)",
      "border-width": "data(borderWidth)",
